{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "enuesaa/laravel-livewire-v4-memos-app",
    "type": "project",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^4.0@beta"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "post-install-cmd": "Illuminate\\Foundation\\ComposerScripts::postInstall",
        "post-update-cmd": "Illuminate\\Foundation\\ComposerScripts::postUpdate",
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "format": "pint --preset psr12"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.25"
    }
}
