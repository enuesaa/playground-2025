{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "azure-web-staticapp-try": {
      "appLocation": ".",
      "outputLocation": "dist",
      "run": "pnpm dev",
      "appBuildCommand": "pnpm build",
      "appDevserverUrl": "http://localhost:3000",
      "apiLocation": "apipython",
      "apiLanguage": "python",
      "apiVersion": "3.10",
      "apiBuildCommand": "uv pip freeze > requirements.txt",
      "swaConfigLocation": "."
    }
  }
}